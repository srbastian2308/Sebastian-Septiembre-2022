{
  "name": "test-talana-frontend",
  "version": "0.1.3",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "test": "vitest --dom",
    "tnw": "vitest run --dom",
    "coverage": "vitest run --coverage --dom",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.5",
    "@vueuse/components": "^8.6.0",
    "@vueuse/core": "^8.6.0",
    "axios": "^0.27.2",
    "bootstrap": "^5.1.3",
    "pinia": "^2.0.13",
    "vue": "^3.2.33",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/bootstrap": "^5.1.10",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^16.11.27",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.0-rc.21",
    "@vue/tsconfig": "^0.1.3",
    "c8": "^7.11.2",
    "eslint": "^8.5.0",
    "eslint-plugin-vue": "^8.2.0",
    "happy-dom": "^3.2.0",
    "jsdom": "^19.0.0",
    "sass": "^1.50.1",
    "start-server-and-test": "^1.14.0",
    "typescript": "~4.6.3",
    "vite": "^2.9.5",
    "vitest": "^0.9.3",
    "vue-tsc": "^0.34.7"
  }
}
